[
  {
    "option": "pinia",
    "templateSyntax": "Liquid",
    "templatePath": "./.vz/templates/samples/vue.pinia/{{store}}.ts.sbn",
    "variables": [
      {
        "name": "store",
        "type": "String"
      }
    ],
    "output": "./src/stores/{{store___camel_case}}.ts"
  },
  {
    "option": "vue.crud",
    "templateSyntax": "Liquid",
    "templatePath": "./.vz/templates/vue/crud/",
    "variables": [
      {
        "name": "path",
        "type": "String",
        "defaultValue": "/api/identity/users"
      },
      {
        "name": "swagger",
        "type": "JsonFile",
        "defaultValue": "./swagger.json"
      }
    ]
  }
]